<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
    
<struts>
	<package name="instock" extends="json-default" namespace="/instock">  
        <action name="list" class="com.mbusiness.action.InstockListAction" method="list">
            <result type="json"></result>
        </action> 
        <action name="add" class="com.mbusiness.action.InstockAddAction" method="add">
            <result type="json"></result>
            <result name="input" type="json"></result>
        </action>
        <action name="delete" class="com.mbusiness.action.InstockDeleteAction" method="delete">
            <result type="json"></result>
            <result name="input" type="json"></result>
        </action> 
        <action name="detaillist" class="com.mbusiness.action.InstockdetailListAction" method="list">
            <result type="json"></result>
        </action> 
        <action name="detailadd" class="com.mbusiness.action.InstockdetailAddAction" method="add">
            <result type="json"></result>
            <result name="input" type="json"></result>
        </action>
        <action name="detaildelete" class="com.mbusiness.action.InstockdetailDeleteAction" method="delete">
            <result type="json"></result>
            <result name="input" type="json"></result>
        </action>
    </package> 
</struts>
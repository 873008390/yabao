<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
    
<struts>
	<package name="department" extends="json-default" namespace="/department">  
        <action name="list" class="com.mbusiness.action.DepartmentListAction" method="list">
            <result type="json"></result>
        </action> 
        <action name="add" class="com.mbusiness.action.DepartmentAddAction" method="add">
            <result type="json"></result>
            <result name="input" type="json"></result>
        </action>
        <action name="delete" class="com.mbusiness.action.DepartmentDeleteAction" method="delete">
            <result type="json"></result>
            <result name="input" type="json"></result>
        </action>
        <action name="syn" class="com.mbusiness.action.DepartmentsynAction" method="syn">
            <result type="json"></result>
            <result name="input" type="json"></result>
        </action>
        <action name="exportDepartmentExcel" class="com.mbusiness.action.ExportDepartmentExcelAction" method="export">
            <result type="json"></result>
        </action> 
    </package> 
</struts>
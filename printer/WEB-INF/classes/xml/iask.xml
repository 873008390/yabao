<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
    
<struts>
	<package name="iask" extends="json-default" namespace="/iask">  
        <action name="list" class="com.mbusiness.action.IaskListAction" method="list">
            <result type="json"></result>
        </action> 
        <action name="add" class="com.mbusiness.action.IaskAddAction" method="add">
            <result type="json"></result>
            <result name="input" type="json"></result>
        </action>
        <action name="delete" class="com.mbusiness.action.IaskDeleteAction" method="delete">
            <result type="json"></result>
            <result name="input" type="json"></result>
        </action>
        <action name="onlineadd" class="com.mbusiness.action.IaskonlineAddAction" method="add">
            <result type="json"></result>
            <result name="input" type="json"></result>
        </action>
        <action name="applist" class="com.mbusiness.action.IaskAppListAction" method="list">
            <result type="json"></result>
        </action> 
        <action name="appdelete" class="com.mbusiness.action.IaskAppDeleteAction" method="delete">
            <result type="json"></result>
            <result name="input" type="json"></result>
        </action>
    </package> 
</struts>
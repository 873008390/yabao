<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
    
<struts>
	<package name="productunit" extends="json-default" namespace="/productunit">  
        <action name="list" class="com.mbusiness.action.ProductunitListAction" method="list">
            <result type="json"></result>
        </action> 
        <action name="add" class="com.mbusiness.action.ProductunitAddAction" method="add">
            <result type="json"></result>
            <result name="input" type="json"></result>
        </action>
        <action name="delete" class="com.mbusiness.action.ProductunitDeleteAction" method="delete">
            <result type="json"></result>
            <result name="input" type="json"></result>
        </action>
    </package> 
</struts>